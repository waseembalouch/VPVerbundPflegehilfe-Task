{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost"
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/app/data/todoapp.db"
  },
  "Jwt": {
    "Issuer": "TodoApp",
    "Audience": "TodoApp",
    "ExpirationHours": 24
  }
}
